program
	array : Integer[5];
	sumfun(arr : Integer[]) : Integer is 
	i : Integer;
	sum : Integer;
	begin
		i := 4;
		while (i >= 0) do
			sum := (sum + arr[i]);
			i := (i-1);
		end while;

		return sum;
	end;

begin
	array[0]:=8;
	array[1]:=4;
	array[2]:=1;
	array[3]:=1;
	
	print sumfun(array);
end;
